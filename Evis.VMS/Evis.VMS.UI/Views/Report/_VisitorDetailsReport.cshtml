  <div class="col-lg-12">
        <div class="x_panel">
            <div class="x_title">
                <div class="col-xs-12">
                    <h2>Visitor Details</h2>
                    <div class="pull-right">
                        <button type="button" class="btn btn-success">Excel <i class="fa fa-file-excel-o"></i></button>
                        <button type="button" class="btn btn-success">Pdf <i class="fa fa-file-pdf-o"></i></button>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
                <form class="form-horizontal form-label-left">
                    <table class="table jambo_table">
                        <thead>
                            <tr>
                                <th style="width: 1%">#</th>
                                <th style="width: 12%"> <a href="#" onclick="DataGrid.Sort('VisitorName')" class="sortCol">Visitor Name</a> </th>
                                <th style="width: 12%"> <a href="#" onclick="DataGrid.Sort('Building')" class="sortCol">Building</a></th>
                                <th style="width: 12%"> <a href="#" onclick="DataGrid.Sort('Gate')" class="sortCol">Gate</a></th>
                                <th style="width: 12%"> <a href="#" onclick="DataGrid.Sort('Security')" class="sortCol">Security</a></th>
                                <th style="width: 12%"><a href="#" onclick="DataGrid.Sort('CheckIn')" class="sortCol">Check In</a></th>
                                <th style="width: 12%"><a href="#" onclick="DataGrid.Sort('CheckOut')" class="sortCol">Check Out</a></th>
                                <th style="width: 12%"><a href="#" onclick="DataGrid.Sort('ContactNumber')" class="sortCol">Contact Number</a></th>
                                <th style="width: 20%"><a href="#" onclick="DataGrid.Sort('VisitDetails')" class="sortCol">Visit Details</a></th>
                            </tr>
                        </thead>
                        <tbody data-bind="foreach:DataGrid.DataRows">
                            <tr>
                                <td scope="row" data-bind="text: $index() +1"></td>
                                <td data-bind="text: VisitorName"></td>
                                <td data-bind="text: Building"></td>
                                <td data-bind="text: Gate"></td>
                                <td data-bind="text: Security"></td>
                                <td data-bind="text: CheckIn"></td>
                                <td data-bind="text: CheckOut"></td>
                                <td data-bind="text: ContactNumber"></td>
                                <td data-bind="text: VisitDetails"></td>
                            </tr>
                        </tbody>
                        <tr class="blank_row" data-bind="visible: (DataGrid.DataRows().length == 0)"><td colspan="10">No data, Add new records.</td></tr>
                    </table>
                    @Html.Partial("_Pager")
                </form>
        </div>
